{
  "name": "@cognitive-workspace/test-utils",
  "version": "0.1.0",
  "description": "Shared testing utilities for the Cognitive Workspace",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
