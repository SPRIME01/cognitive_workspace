    "@testing-library/user-event": "^14.4.3",
    "msw": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.3.3"
  }
}
